{
  "ConnectionStrings": {
    "Default": "Data Source=Wheel.WebApi.Host.db",
    "RabbitMq": "127.0.0.1",
    "Redis": "127.0.0.1:6379"
  },
  "Aspire": {
    "StackExchange": {
      "Redis": {
        "ConfigurationOptions": {
          "ConnectTimeout": 3000,
          "ConnectRetry": 2
        },
        "HealthChecks": true,
        "Tracing": true
      }
    },
    "RabbitMQ": {
      "Client": {
        "HealthChecks": true
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "MailKit": {
    "SenderName": "Wheel",
    "Host": "",
    "Prot": 465,
    "UserName": "",
    "Password": "",
    "UseSsl": true
  }
}
